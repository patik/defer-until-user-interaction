{
    "compilerOptions": {
        "target": "es2016",
        "lib": ["dom", "esnext"],
        "jsx": "preserve",
        "module": "esnext",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "declaration": true,
        "declarationMap": true,
        "sourceMap": true,
        "outDir": "dist",
        "rootDir": ".",
        "baseUrl": "."
    }
}
